<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
    <link rel="stylesheet" href="casino.css">
  </head>
  <body>

    <div class="col-sm-6">
            <h3>Casino owner access</h3>
            <ul class="messages">
            <% for(var i=0; i<casino.length; i++) {%>
              <li class="message">

                <span>Win:<%= casino[i].won %></span>
                <span>Loss:<%= casino[i].loss %></span>
                <span>Casino Money:<%=casino[i].casinoMoney %></span>
                <span><i class="fa fa-thumbs-up" aria-hidden="true"></i></span>
                <span><i class="fa fa-thumbs-down" aria-hidden="true"></i></span>
                <span><i class="fa fa-trash" aria-hidden="true"></i></span>
              </li>
            <% } %>
            </ul>
            <section>
              <a href="/logout">Logout</a>
            </section>

            <script type="text/javascript">
  //           setTimeout(function(){
  //     window.location.reload();
  // },2000);
            </script>
  </body>
</html>
